<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ApiLogin.
    -->
    <changeSet id="20190723021532-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="api_client_id"
                                 baseTableName="api_login"
                                 constraintName="fk_api_login_api_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="api_client"/>

        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="api_login"
                                 constraintName="fk_api_login_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="api_credential"/>

    </changeSet>
</databaseChangeLog>
